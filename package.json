{
  "name": "dicty-frontpage",
  "version": "0.0.0-semantically-released",
  "private": true,
  "engines": {
    "node": ">= 16.13.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test-coverage": "react-scripts test --coverage",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .jsx,.js,.ts,.tsx src/",
    "tsc": "tsc",
    "styleguide": "FAST_REFRESH=false styleguidist server",
    "styleguide:build": "FAST_REFRESH=false styleguidist build",
    "semantic-release": "semantic-release",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "snyk-protect": "snyk protect",
    "postinstall": "husky install",
    "start:mock": "REACT_APP_MOCK_SERVER=on react-scripts start"
  },
  "dependencies": {
    "@apollo/client": "^3.6.8",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "apollo3-cache-persist": "^0.14.0",
    "core-js": "^3.23.1",
    "dicty-components-header-footer": "dictybase/dicty-components-header-footer#11.0.1",
    "dicty-components-login": "dictybase/dicty-components-login#4.1.1",
    "dicty-components-navbar": "github:dictyBase/dicty-components-navbar#2.0.0",
    "dicty-components-page-editor": "dictybase/dicty-components-page-editor#3.1.2",
    "dicty-graphql-schema": "dictyBase/dicty-graphql-schema",
    "dicty-hooks": "dictybase/dicty-hooks#3.0.0",
    "font-awesome": "^4.7.0",
    "fontsource-roboto": "^4.0.0",
    "graphql": "^16.5.0",
    "graphql-tag": "^2.12.6",
    "history": "^4.10.1",
    "jwt-decode": "^3.0.0",
    "localforage": "^1.10.0",
    "react": "^17.0.2",
    "react-app-polyfill": "^3.0.0",
    "react-dom": "^16.13.1",
    "react-ga": "^3.3.1",
    "react-helmet": "^6.1.0",
    "react-loading-skeleton": "^3.1.0",
    "react-responsive-carousel": "^3.2.23",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "twitter-fetcher": "^18.0.4",
    "typescript": "4.7.3"
  },
  "devDependencies": {
    "@material-ui/styles": "^4.11.4",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.4",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.2.1",
    "@types/jest": "^27.5.1",
    "@types/jwt-decode": "^3.1.0",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.3",
    "@types/react-fontawesome": "^1.6.4",
    "@types/react-helmet": "^6.1.5",
    "@types/react-redux": "^7.1.24",
    "@types/react-router-dom": "^5.3.3",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "8.0.1",
    "lint-staged": "^13.0.2",
    "msw": "^0.42.1",
    "prettier": "^2.7.1",
    "react-docgen-typescript": "^2.2.2",
    "react-styleguidist": "^11.2.0",
    "react-test-renderer": "^17.0.2",
    "semantic-release": "^19.0.3",
    "snyk": "^1.953.0",
    "source-map-explorer": "^2.5.2",
    "wait-for-expect": "^3.0.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/index.js"
    ],
    "resetMocks": false
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --no-semi --trailing-comma all --jsx-bracket-same-line true --write",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "prettier",
      "plugin:jest-dom/recommended"
    ],
    "plugins": [
      "prettier",
      "jest-dom"
    ],
    "rules": {
      "import/exports-last": [
        "error"
      ]
    }
  },
  "snyk": true,
  "msw": {
    "workerDirectory": "public"
  }
}
