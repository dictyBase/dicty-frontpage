{
  "name": "dicty-frontpage",
  "version": "0.0.0-semantically-released",
  "private": true,
  "engines": {
    "node": "10.16.0"
  },
  "scripts": {
    "prettier": "prettier --version",
    "flow": "flow",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test-coverage": "react-scripts test --env=jsdom --coverage",
    "eject": "react-scripts eject",
    "cz": "git-cz",
    "semantic-release": "semantic-release",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "local-ci-eric": "IMAGE_TAG=$(git symbolic-ref -q HEAD | sed -e 's|^refs/heads/||' -e 's~/~-~')-$(git rev-parse --short HEAD) skaffold run -p ericdev",
    "local-ci-sidd": "IMAGE_TAG=$(git symbolic-ref -q HEAD | sed -e 's|^refs/heads/||' -e 's~/~-~')-$(git rev-parse --short HEAD) skaffold run -p devsidd",
    "local-ci-staging": "IMAGE_TAG=staging-$(git rev-parse --short HEAD) skaffold run"
  },
  "dependencies": {
    "@convertkit/slate-lists": "^0.2.2",
    "@material-ui/core": "^4.2.1",
    "@material-ui/icons": "^4.2.1",
    "connected-react-router": "^6.5.0",
    "core-js": "^3.1.4",
    "dicty-components-header-footer": "github:dictyBase/dicty-components-header-footer#10.0.1",
    "dicty-components-login": "github:dictyBase/dicty-components-login#3.0.1",
    "dicty-components-navbar": "github:dictyBase/dicty-components-navbar#2.0.0",
    "dicty-components-redux": "github:dictybase-playground/dicty-components-redux#v2.3.0",
    "font-awesome": "^4.7.0",
    "get-video-id": "^3.1.4",
    "history": "^4.9.0",
    "immutable": "^3.8.2",
    "is-url": "^1.2.4",
    "jwt-decode": "^2.2.0",
    "react": "^16.8.6",
    "react-app-polyfill": "^1.0.1",
    "react-color": "^2.14.1",
    "react-dom": "^16.8.6",
    "react-fontawesome": "^1.6.1",
    "react-ga": "^2.6.0",
    "react-helmet": "^5.2.1",
    "react-loading-skeleton": "^1.1.2",
    "react-redux": "^7.1.0",
    "react-responsive-carousel": "^3.1.49",
    "react-router-dom": "^5.0.1",
    "react-scripts": "^3.0.1",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "slate": "^0.44.13",
    "slate-deep-table": "^0.8.0",
    "slate-html-serializer": "^0.8.10",
    "slate-react": "^0.21.20",
    "slate-schema-violations": "^0.1.39",
    "twitter-fetcher": "^18.0.2",
    "typeface-lato": "0.0.54",
    "typeface-merriweather": "0.0.72",
    "typeface-montserrat": "0.0.54",
    "typeface-roboto": "0.0.54",
    "typeface-roboto-condensed": "0.0.54",
    "typeface-roboto-mono": "0.0.54",
    "typeface-roboto-slab": "0.0.72"
  },
  "devDependencies": {
    "@semantic-release/git": "^7.0.16",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "flow-bin": "^0.108.0",
    "flow-typed": "^2.6.1",
    "husky": "^3.0.5",
    "jest-enzyme": "^7.1.1",
    "jss": "^9.8.7",
    "lint-staged": "^9.2.5",
    "prettier": "^1.18.2",
    "react-styleguidist": "^9.1.16",
    "react-test-renderer": "^16.8.6",
    "semantic-release": "^15.13.24",
    "source-map-explorer": "^2.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && CI=true npm run test",
      "post-commit": "npm run local-ci-eric"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --no-semi --trailing-comma all --jsx-bracket-same-line true --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "noCi": true,
    "tagFormat": "${version}",
    "verifyConditions": [
      "@semantic-release/git"
    ],
    "publish": [
      "@semantic-release/github"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  }
}
