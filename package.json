{
  "name": "dicty-frontpage",
  "version": "0.0.0-semantically-released",
  "private": true,
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test-coverage": "CI=true react-scripts test --env=jsdom --coverage",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .jsx,.js,.ts,.tsx src/",
    "tsc": "tsc",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "semantic-release": "semantic-release",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "local-ci-eric": "IMAGE_TAG=$(git symbolic-ref -q HEAD | sed -e 's|^refs/heads/||' -e 's~/~-~')-$(git rev-parse --short HEAD) skaffold run -p ericdev",
    "local-ci-sidd": "IMAGE_TAG=$(git symbolic-ref -q HEAD | sed -e 's|^refs/heads/||' -e 's~/~-~')-$(git rev-parse --short HEAD) skaffold run -p devsidd",
    "snyk-protect": "snyk protect"
  },
  "dependencies": {
    "@apollo/client": "^3.1.3",
    "@material-ui/core": "^4.10.2",
    "connected-react-router": "^6.6.1",
    "core-js": "^3.6.5",
    "dicty-components-header-footer": "github:dictyBase/dicty-components-header-footer#10.1.0",
    "dicty-components-login": "github:dictyBase/dicty-components-login#3.0.1",
    "dicty-components-navbar": "github:dictyBase/dicty-components-navbar#2.0.0",
    "dicty-components-page-editor": "github:dictyBase/dicty-components-page-editor#1.0.3",
    "font-awesome": "^4.7.0",
    "graphql": "^15.3.0",
    "history": "^4.10.1",
    "jwt-decode": "^2.2.0",
    "react": "^16.13.1",
    "react-app-polyfill": "^1.0.6",
    "react-dom": "^16.13.1",
    "react-fontawesome": "^1.7.1",
    "react-ga": "^3.1.2",
    "react-helmet": "^6.1.0",
    "react-loading-skeleton": "^2.1.1",
    "react-redux": "^7.2.1",
    "react-responsive-carousel": "^3.2.9",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "snyk": "^1.371.1",
    "twitter-fetcher": "^18.0.4",
    "typeface-roboto": "0.0.75"
  },
  "devDependencies": {
    "@semantic-release/git": "^9.0.0",
    "@types/jest": "^26.0.9",
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.3",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "jest-enzyme": "^7.1.2",
    "jss": "^10.3.0",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "react-scripts": "^3.4.0",
    "react-styleguidist": "^11.0.8",
    "react-test-renderer": "^16.13.1",
    "semantic-release": "^17.1.1",
    "source-map-explorer": "^2.4.2",
    "typescript": "^3.9.7"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --no-semi --trailing-comma all --jsx-bracket-same-line true --write",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "snyk": true
}
