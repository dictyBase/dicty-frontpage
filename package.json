{
  "name": "dicty-frontpage",
  "version": "0.0.0-semantically-released",
  "private": true,
  "engines": {
    "node": ">= 14"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test-coverage": "react-scripts test --coverage",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .jsx,.js,.ts,.tsx src/",
    "tsc": "tsc",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "semantic-release": "semantic-release",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "snyk-protect": "snyk protect",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@apollo/client": "^3.3.11",
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@material-ui/core": "^4.11.3",
    "core-js": "^3.9.1",
    "dicty-components-header-footer": "github:dictyBase/dicty-components-header-footer#10.2.1",
    "dicty-components-login": "github:dictyBase/dicty-components-login#3.1.1",
    "dicty-components-navbar": "github:dictyBase/dicty-components-navbar#2.0.0",
    "dicty-components-page-editor": "github:dictyBase/dicty-components-page-editor#1.3.1",
    "dicty-hooks": "dictyBase/dicty-hooks#1.1.1",
    "font-awesome": "^4.7.0",
    "fontsource-roboto": "^3.1.5",
    "graphql": "^15.5.0",
    "graphql-tag": "^2.11.0",
    "history": "^4.10.1",
    "jwt-decode": "^3.0.0",
    "react": "^16.13.1",
    "react-app-polyfill": "^2.0.0",
    "react-dom": "^16.13.1",
    "react-ga": "^3.3.0",
    "react-helmet": "^6.1.0",
    "react-loading-skeleton": "^2.1.1",
    "react-responsive-carousel": "^3.2.13",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.2",
    "twitter-fetcher": "^18.0.4",
    "typescript": "4.2.3"
  },
  "devDependencies": {
    "@semantic-release/git": "^9.0.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/react-hooks": "^5.1.0",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.20",
    "@types/jwt-decode": "^3.1.0",
    "@types/node": "^14.14.34",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/react-fontawesome": "^1.6.4",
    "@types/react-helmet": "^6.1.0",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jest-dom": "^3.6.5",
    "eslint-plugin-prettier": "^3.2.0",
    "husky": "5",
    "lint-staged": "^10.5.4",
    "prettier": "^2.0.5",
    "react-docgen-typescript": "^1.21.0",
    "react-styleguidist": "^11.1.5",
    "react-test-renderer": "^16.13.1",
    "semantic-release": "^17.4.2",
    "snyk": "^1.502.0",
    "source-map-explorer": "^2.5.2",
    "wait-for-expect": "^3.0.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "resetMocks": false
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --no-semi --trailing-comma all --jsx-bracket-same-line true --write",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "prettier",
      "plugin:jest-dom/recommended"
    ],
    "plugins": [
      "prettier",
      "jest-dom"
    ],
    "rules": {
      "import/exports-last": [
        "error"
      ]
    }
  },
  "snyk": true
}
