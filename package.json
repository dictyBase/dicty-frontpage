{
  "name": "dicty-frontpage",
  "version": "0.0.0-semantically-released",
  "private": true,
  "engines": {
    "node": ">= 14"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test-coverage": "react-scripts test --coverage",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .jsx,.js,.ts,.tsx src/",
    "tsc": "tsc",
    "styleguide": "FAST_REFRESH=false styleguidist server",
    "styleguide:build": "FAST_REFRESH=false styleguidist build",
    "semantic-release": "semantic-release",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "snyk-protect": "snyk protect",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@apollo/client": "^3.4.16",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "apollo3-cache-persist": "^0.9.1",
    "core-js": "^3.19.1",
    "dicty-components-header-footer": "dictybase/dicty-components-header-footer#11.0.0",
    "dicty-components-login": "dictybase/dicty-components-login#4.1.1",
    "dicty-components-navbar": "github:dictyBase/dicty-components-navbar#2.0.0",
    "dicty-components-page-editor": "dictybase/dicty-components-page-editor#3.1.2",
    "dicty-graphql-schema": "dictybase/dicty-graphql-schema#1.1.4",
    "dicty-hooks": "dictybase/dicty-hooks#3.0.0",
    "font-awesome": "^4.7.0",
    "fontsource-roboto": "^4.0.0",
    "graphql": "^15.7.2",
    "graphql-tag": "^2.12.6",
    "history": "^4.10.1",
    "jwt-decode": "^3.0.0",
    "localforage": "^1.10.0",
    "react": "^16.13.1",
    "react-app-polyfill": "^2.0.0",
    "react-dom": "^16.13.1",
    "react-ga": "^3.3.0",
    "react-helmet": "^6.1.0",
    "react-loading-skeleton": "^3.0.1",
    "react-responsive-carousel": "^3.2.22",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^4.0.2",
    "twitter-fetcher": "^18.0.4",
    "typescript": "4.4.4"
  },
  "devDependencies": {
    "@material-ui/styles": "^4.11.4",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.1",
    "@semantic-release/github": "^7.2.3",
    "@semantic-release/npm": "^7.1.1",
    "@semantic-release/release-notes-generator": "^9.0.2",
    "@testing-library/dom": "^8.11.1",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.0.2",
    "@types/jwt-decode": "^3.1.0",
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "@types/react-fontawesome": "^1.6.4",
    "@types/react-helmet": "^6.1.4",
    "@types/react-redux": "^7.1.20",
    "@types/react-router-dom": "^5.3.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest-dom": "^3.9.2",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "7.0.4",
    "lint-staged": "^11.2.6",
    "prettier": "^2.4.1",
    "react-docgen-typescript": "^2.1.1",
    "react-styleguidist": "^11.1.7",
    "react-test-renderer": "^16.13.1",
    "semantic-release": "^17.4.7",
    "snyk": "^1.759.0",
    "source-map-explorer": "^2.5.2",
    "wait-for-expect": "^3.0.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "resetMocks": false
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --no-semi --trailing-comma all --jsx-bracket-same-line true --write",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "prettier",
      "plugin:jest-dom/recommended"
    ],
    "plugins": [
      "prettier",
      "jest-dom"
    ],
    "rules": {
      "import/exports-last": [
        "error"
      ]
    }
  },
  "snyk": true
}
