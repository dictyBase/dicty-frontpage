{
  "name": "dicty-frontpage",
  "version": "0.0.0-semantically-released",
  "private": true,
  "engines": {
    "node": "10.17.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test-coverage": "CI=true react-scripts test --env=jsdom --coverage",
    "eject": "react-scripts eject",
    "eslint": "eslint --ext .jsx,.js src/",
    "prettier": "prettier --version",
    "flow": "flow",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "cz": "git-cz",
    "semantic-release": "semantic-release",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "local-ci-eric": "IMAGE_TAG=$(git symbolic-ref -q HEAD | sed -e 's|^refs/heads/||' -e 's~/~-~')-$(git rev-parse --short HEAD) skaffold run -p ericdev",
    "local-ci-sidd": "IMAGE_TAG=$(git symbolic-ref -q HEAD | sed -e 's|^refs/heads/||' -e 's~/~-~')-$(git rev-parse --short HEAD) skaffold run -p devsidd"
  },
  "dependencies": {
    "@material-ui/core": "^4.10.0",
    "connected-react-router": "^6.6.1",
    "core-js": "^3.6.5",
    "dicty-components-header-footer": "github:dictyBase/dicty-components-header-footer#10.1.0",
    "dicty-components-login": "github:dictyBase/dicty-components-login#3.0.1",
    "dicty-components-navbar": "github:dictyBase/dicty-components-navbar#2.0.0",
    "dicty-components-page-editor": "github:dictyBase/dicty-components-page-editor#1.0.3",
    "dicty-components-redux": "github:dictybase-playground/dicty-components-redux#v2.3.0",
    "font-awesome": "^4.7.0",
    "history": "^4.10.1",
    "jwt-decode": "^2.2.0",
    "react": "^16.13.1",
    "react-app-polyfill": "^1.0.6",
    "react-dom": "^16.13.1",
    "react-fontawesome": "^1.7.1",
    "react-ga": "^2.7.0",
    "react-helmet": "^6.0.0",
    "react-loading-skeleton": "^2.0.1",
    "react-redux": "^7.2.0",
    "react-responsive-carousel": "^3.2.7",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "twitter-fetcher": "^18.0.4",
    "typeface-roboto": "0.0.75"
  },
  "devDependencies": {
    "@semantic-release/git": "^9.0.0",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "flow-bin": "^0.125.1",
    "flow-typed": "^3.1.0",
    "husky": "^4.2.5",
    "jest-enzyme": "^7.1.2",
    "jss": "^10.0.4",
    "lint-staged": "^10.2.7",
    "prettier": "^1.19.1",
    "react-styleguidist": "^11.0.8",
    "react-test-renderer": "^16.13.1",
    "semantic-release": "^17.0.8",
    "source-map-explorer": "^2.4.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/components/**/*.{js,jsx,ts,tsx}",
      "src/utils/**/*.{js,jsx,ts,tsx}"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --no-semi --trailing-comma all --jsx-bracket-same-line true --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "releaseRules": [
            {
              "type": "docs",
              "release": "patch"
            },
            {
              "type": "perf",
              "release": "patch"
            },
            {
              "type": "test",
              "release": "patch"
            },
            {
              "type": "chore",
              "release": "patch"
            },
            {
              "type": "refactor",
              "release": "patch"
            }
          ]
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ],
    "ci": true,
    "tagFormat": "${version}",
    "verifyConditions": [
      "@semantic-release/git"
    ],
    "publish": [
      "@semantic-release/github"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  }
}
